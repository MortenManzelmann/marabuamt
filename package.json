{
  "name": "marabuamt",
  "version": "1.0.0",
  "description": "Homepage of the mighty marabuians",
  "main": "server.js",
  "scripts": {
    "build": "node node_modules/tsc/bin/tsc",
    "dev": "node node_modules/tsc/bin/tsc --watch & nodemon dist/server.js",
    "watch:sass": "node ./node_modules/node-sass/bin/node-sass -w src/styles/main.scss public/css/main.css",
    "lint": "eslint src --ext ts",
    "start": "yarn build & nodemon dist/server.js",
    "build:dev": "node ./node_modules/webpack-cli/bin/cli.js --devtool eval --config webpack.config.js",
    "watch:dev": "node ./node_modules/webpack-cli/bin/cli.js --devtool eval-source-map --watch --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MortenManzelmann/marabuamt.git"
  },
  "keywords": [
    "marabu",
    "amt",
    "marabuamt",
    "uff",
    "und",
    "zu",
    "idioten"
  ],
  "author": "Morten Manzelmann",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MortenManzelmann/marabuamt/issues"
  },
  "homepage": "https://github.com/MortenManzelmann/marabuamt#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.18.3",
    "connect-flash": "^0.1.1",
    "dev": "^0.1.3",
    "eslint": "^6.3.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "express": "^4.16.3",
    "express-messages": "^1.0.1",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "mongoose": "^5.3.1",
    "node-sass": "^4.12.0",
    "pug": "^2.0.3",
    "reset-css": "^4.0.1",
    "sass-loader": "^8.0.0",
    "tsc": "^1.20150623.0",
    "typescript": "^3.6.3",
    "typescript-eslint-parser": "^22.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "babel-eslint": "^10.0.3",
    "css-loader": "^3.2.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8"
  }
}
